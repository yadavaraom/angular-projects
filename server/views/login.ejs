<section class="login-block">
        <!-- Container-fluid starts -->
        <div class="container">
          <div class="row">
            <div class="col-sm-12">
              <!-- Authentication card start -->
              <form class="md-float-material form-material" method="post">
                <div class="text-center" style="font-size: 1.75rem; margin: 0px 30%;">
                        <% if(locals.error) { %>
                            <div class="alert alert-danger"><%= error %></div>
                        <% } %>
                        <% if(locals.success) { %>
                            <div class="alert alert-success"><%= success %></div>
                        <% } %>
                  <!-- <img src="./img/logo.png" alt="logo.png"> -->
                </div>
                <div class="auth-box card">
                  <div class="card-block">
                    <div class="row m-b-20">
                      <div class="col-md-12">
                        <h2 style="font-size: 2.75rem" class="text-center">Sign In</h2>
                      </div>
                    </div>
                    <div class="row m-t-30">
                        
                        </div>
                        <div class="row m-t-30">
                        
                            </div>
            
                    <div class="form-group form-primary">
                            <input type="text" name="username" id="username" value="<%= locals.username || '' %>" class="form-control" required=""/>
                        <span class="form-bar"></span>
                      <label class="float-label">User Name</label>
                    </div>
                    <div class="form-group form-primary">
                      
                      <input type="password" name="password" id="password" class="form-control" required=""/>


                      <span class="form-bar"></span>
                      <label class="float-label">Password</label>
                    </div>
                    <div class="row m-t-30">

                    </div>
                    <div class="row m-t-30">
                      <div class="col-md-12">
                        <button type="submit" class="btn btn-primary btn-md btn-block text-center m-b-20 ripple light">Sign in</button>
                      </div>
                    </div>
                    <div class="row m-t-30">
                        
                        </div>
    
                    <hr/>
                                        <div class="row m-t-30">
                        
                    </div>

                  </div>
                </div>
              </form>
              <!-- end of form -->
            </div>
            <!-- end of col-sm-12 -->
          </div>
          <!-- end of row -->
        </div>
        <!-- end of container-fluid -->
      </section>


